<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="37.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GUI.ShelterList">
    <children>
        <HBox layoutY="2.0" maxWidth="400.0" minWidth="400.0" prefWidth="400.0">
            <children>
                <Text fx:id="shelterIDText" strokeType="OUTSIDE" strokeWidth="0.0" text="ID">
                    <font>
                        <Font size="18.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Text>
                <Text fx:id="shelterName" strokeType="OUTSIDE" strokeWidth="0.0" text="Name ">
                    <font>
                        <Font size="18.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="100.0" />
                    </HBox.margin>
                </Text>
                <VBox prefHeight="35.0" prefWidth="79.0">
                    <children>
                        <Text fx:id="shelterEmail" strokeType="OUTSIDE" strokeWidth="0.0" text="Email">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Text>
                    </children>
                    <HBox.margin>
                        <Insets left="100.0" />
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
