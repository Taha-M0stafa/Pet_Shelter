<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane minHeight="1080.0" minWidth="1920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GUI.RegisterView">
   <center>
      <VBox alignment="CENTER" spacing="10.0" styleClass="root" prefWidth="300.0" prefHeight="500.0">
      <AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="15.0" layoutY="95.0">
               <children>
                  <TextField fx:id="addressText1" prefHeight="43.0" prefWidth="225.0" promptText="Address" style="-fx-border-color: black; -fx-border-width: 1;" />
                  <TextField fx:id="email" prefHeight="40.0" prefWidth="220.0" promptText="Email" style="-fx-border-width: 1; -fx-border-color: black;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="username" prefHeight="40.0" prefWidth="220.0" promptText="Username" style="-fx-border-width: 1; -fx-border-color: black;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <PasswordField fx:id="password" prefHeight="40.0" prefWidth="220.0" promptText="Password" style="-fx-border-color: black; -fx-border-width: 1;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </PasswordField>
                  <TextField fx:id="textID" prefHeight="43.0" prefWidth="225.0" promptText="ID" style="-fx-border-color: black; -fx-border-width: 1;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="addressText" prefHeight="43.0" prefWidth="225.0" promptText="Address" style="-fx-border-color: black; -fx-border-width: 1;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="PhoneNumText" prefHeight="43.0" prefWidth="225.0" promptText="Phone Number" style="-fx-border-color: black; -fx-border-width: 1;">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="createAccount" mnemonicParsing="false" onAction="#onCreateAccount" text="Create Account">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Button fx:id="returnButton" layoutX="7.0" layoutY="1.0" mnemonicParsing="false" onAction="#onReturnButton" text="Return" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
