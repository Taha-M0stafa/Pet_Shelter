<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="borderPane" minHeight="1080.0" minWidth="1920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GUI.ProgramView">
   <top>
      <AnchorPane fx:id="topAnchorPane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #FEDCAE;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="profileButton" layoutX="101.0" layoutY="75.0" mnemonicParsing="false" text="Profile" />
            <Button fx:id="adoptButton" layoutX="320.0" layoutY="75.0" mnemonicParsing="false" text="Adopt" />
            <Button fx:id="adminButton" layoutX="1779.0" layoutY="75.0" mnemonicParsing="false" text="Admin" />
            <Button fx:id="returnButton" layoutX="7.0" layoutY="1.0" mnemonicParsing="false" onAction="#onReturn" text="Return" />
         </children>
      </AnchorPane>
   </top>
   <left>
      <AnchorPane fx:id="leftAnchorPane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #FEDCAE;" BorderPane.alignment="CENTER_LEFT" />
   </left>
</BorderPane>
